{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/gpt", "dest": "/server.js" },
    { "src": "/api/tts", "dest": "/server.js" },
    { "src": "/assets/(.*)", "dest": "/public/assets/$1" },
    { "src": "/(.*)", "dest": "/server.js" }
  ]
}
